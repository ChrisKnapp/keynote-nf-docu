\pard\f0\fs20\lang1033\par
\cf1\b\fs36\lang3082 Dividers. Examples\cf2\lang1033\par
\cf0\b0\fs20\par
\fs18 Predefined dividers:\par
\par

\pard\sa100\b Clipboard Capture:\par

\pard\li120 {\b0{\field{\*\fldinst{HYPERLINK "file:///*8|581|1695|0|1"}}{\fldrslt{\ul\cf2\cf2\ul ^^^--- %D, %T %| - - -  %S%|^^%%^. . .^}}}}\b0\f0\fs18\par
{{\field{\*\fldinst{HYPERLINK "file:///*8|581|2498|0|2"}}{\fldrslt{\ul\cf2\cf2\ul ^^-- %D %|-  [ %S ] %|--^%%^- -^}}}}\f0\fs18\par
{{\field{\*\fldinst{HYPERLINK "file:///*8|581|3270|0|3"}}{\fldrslt{\ul\cf2\cf2\ul ^^--- %S%d^%%^. . .^}}}}\f0\fs18\par
{{\field{\*\fldinst{HYPERLINK "file:///*8|581|4022|0|4"}}{\fldrslt{\ul\cf2\cf2\ul ^%|^%|- - - - - - - -  %S%d %|- - - -%|^%%^- - -^}}}}\f0\fs18\par
{{\field{\*\fldinst{HYPERLINK "file:///*8|581|4837|0|5"}}{\fldrslt{\ul\cf2\cf2\ul ^^%|- >  %S %d  %|- - - - - - -%|^%|^%%^- -^}}}}\f0\fs18\par
{{\field{\*\fldinst{HYPERLINK "file:///*8|581|5633|0|6"}}{\fldrslt{\ul\cf2\cf2\ul ^^%|=>  %S %d  %|======%|^%|^%%^- - - -^}}}}\f0\fs18\par
{{\field{\*\fldinst{HYPERLINK "file:///*8|581|6426|0|7"}}{\fldrslt{\ul\cf2\cf2\ul ^^- - - - - - - - - - - -^ %|>>   %S %d^%|%%^- - -^}}}}\f0\fs18\par
