\pard\f0\fs20\lang1033\par
\cf1\b\f1\fs36\lang1045 Menu Commands Overview\cf2\f0\fs26\lang1033\par
\cf0\b0\fs20\par
\v\'11I170\'12\v0{\field{\*\fldinst{HYPERLINK "img:170,405,23"}}{\fldrslt {170_Clipb_22feb.png}}}\par
\par
The sections below describe each command available through the \b main menu\b0  in KeyNote.\par
\par

\pard\li360 {{\field{\*\fldinst{HYPERLINK "file:///*8|29|0|0"}}{\fldrslt{\ul\cf2\cf2\ul File menu}}}}\cf3\f0\fs20\par
{\cf0{\field{\*\fldinst{HYPERLINK "file:///*8|46|0|0"}}{\fldrslt{\ul\cf2\cf2\ul Edit menu}}}}\f0\fs20\par
{\cf0{\field{\*\fldinst{HYPERLINK "file:///*8|85|0|0"}}{\fldrslt{\ul\cf2\cf2\ul View menu}}}}\f0\fs20\par
